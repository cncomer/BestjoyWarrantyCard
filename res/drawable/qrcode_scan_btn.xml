<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >

		    <solid android:color="#ff124379"/>
		
		    <stroke android:color="@color/wt" android:width="0.5dip"/>
		    <padding
		        android:bottom="0.5dp"
		        android:left="0.5dp"
		        android:right="0.5dp"
		        android:top="0.5dp" />
		</shape>
    </item>
	<item android:state_pressed="false">
	    <shape>
	    	<solid android:color="#ff1f68ba" />
			<stroke android:color="@color/wt" android:width="0.5dip"/>
		    <padding
		        android:bottom="0.5dp"
		        android:left="0.5dp"
		        android:right="0.5dp"
		        android:top="0.5dp" />
		</shape>
	</item>
</selector>
